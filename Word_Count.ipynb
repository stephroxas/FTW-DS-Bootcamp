      "source": [
        "def counter(song):\n",
        "  list = song.split(\" \")\n",
        "  dict = {}\n",
        "\n",
        "  for i in list:\n",
        "    if i in dict:\n",
        "      dict[i] = dict[i] + 1\n",
        "    else:\n",
        "      dict[i] = 1\n",
        "\n",
        "  return dict"
      ],
      "execution_count": 0,
      "outputs": []
    },
